<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"

    android:layout_height="match_parent"
    tools:context="com.example.sys1.crack_it.Main2Activity">


    <Spinner
        android:id="@+id/branches"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="72dp"
        android:entries="@array/branch"/>
        <!--android:prompt="Select Branch"-->

    <Spinner
        android:id="@+id/year"
        android:entries="@array/year"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/branches"
        android:layout_marginTop="44dp">
        <!--android:prompt="Select Year"-->


    </Spinner>

   <Spinner
        android:id="@+id/company_name"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:entries="@array/company"
        android:layout_below="@+id/year"
        android:layout_marginTop="44dp"/>
       <!-- android:hint="Enter Company Name" />-->

    <Spinner
        android:id="@+id/round_number"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:entries="@array/round"
        android:layout_below="@+id/company_name"
        android:layout_marginTop="48dp">

    </Spinner>

    <Button
        android:id="@+id/Submit"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_below="@+id/round_number"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="61dp"
        android:background="@drawable/check"/>
</RelativeLayout>
